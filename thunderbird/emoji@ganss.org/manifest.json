{
    "manifest_version": 2,
    "name": "Emoji",
    "description": "Add emojis to your e-mails.",
    "version": "1.68.0",
    "author": "Michael Ganss",
    "homepage_url": "https://www.updatestar.com/dl/emoji/emoji.html",
    "applications": {
      "gecko": {
        "id": "emoji@ganss.org",
        "strict_min_version": "68.0"
      }
    },
    "icons": {
      "32": "chrome/skin/icon.svg",
      "48": "chrome/skin/icon.svg",
      "96": "chrome/skin/icon.svg"
    },
    "legacy": {
      "type": "xul",
      "options": {
        "page": "chrome://emoji/content/preferences.xul",
        "open_in_tab": true
      }
    }
  }